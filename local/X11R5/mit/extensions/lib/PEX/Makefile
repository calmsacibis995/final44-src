lintlib::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in c_binding ;\
	do \
	(cd $$i ; echo "linting" for lintlib and lintlib "in $(CURRENT_DIR)/$$i..."; \
	$(MAKE) $(MFLAGS) DESTDIR='$(DESTDIR)' LINTOPTS='$(LINTOPTS)' lintlib); \
	done

