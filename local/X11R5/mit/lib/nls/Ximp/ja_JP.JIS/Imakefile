EUC32 = 0
INSTALLFLAGS = $(INSTDATFLAGS)

#ifdef XlibI18nPath
XLIBI18N_PATH = XlibI18nPath
#else
XLIBI18N_PATH = $(LIBDIR)
#endif
LCFILES = Codeset

AllTarget($(LCFILES))

CppFileTarget($(LCFILES), $(LCFILES).cpp, -DEUC32=$(EUC32), NullParameter)

clean::
	$(RM) $(LCFILES)
depend::

InstallMultiple($(LCFILES),$(XLIBI18N_PATH)/ja_JP.JIS)
