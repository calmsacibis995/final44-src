#include <Server.tmpl>

SRCS = driver.c bank.s

OBJS = driver.o bank.o

INCLUDES = -I. -I../.. -I../../vga -I../../../../include -I$(INCLUDESRC)

NormalLibraryObjectRule()

.s.o:
	$(RM) $@
	$(CPP) $*.s >$*.i
	$(AS) -o $*.o $*.i
	$(RM) $*.i

NormalRelocatableTarget(pvga1, $(OBJS))

DependTarget()

