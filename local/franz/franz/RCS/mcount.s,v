access   ;
symbols  ;
locks    ; strict;
comment  @ * @;


1.1
date     83.03.21.17.01.13;  author jkf;  state Exp;
branches ;
next     ;


desc
@profiling code
@



1.1
log
@Initial revision
@
text
@	.asciz	"$Header$"
# count subroutine called during profiling

.globl	mcount
.globl _mcount
.comm	_countbase,4

_mcount:
mcount:
	movl	(r0),r1
	beql	init
incr:
	incl	(r1)
return:
	rsb
init:
	movl	_countbase,r1
	beql	return
	addl2	$8,_countbase
	movl	(sp),(r1)+
	movl	r1,(r0)
	brb 	incr
@
