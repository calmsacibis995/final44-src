# make script for make-util (Toolchest)

SHELLS = makefile nlorder mkxref

all : $(SHELLS) ALL

#top#

# Generated by makefile 1.43    Fri Feb 22 15:16:26 EST 1985

MAKEFILE = Makefile


MAINS = mkxref.pgm shorten

OBJECTS =  mkxref.pgm.o shorten.o

SOURCES =  mkxref.pgm.c shorten.c

ALL:		$(MAINS)

mkxref.pgm:	mkxref.pgm.o 
	$(CC) $(CFLAGS)  -o mkxref.pgm  mkxref.pgm.o   $(LDFLAGS)

shorten:	shorten.o 
	$(CC) $(CFLAGS)  -o shorten  shorten.o   $(LDFLAGS)


mkxref.pgm.o:	 /usr/include/stdio.h /usr/include/string.h 

shorten.o:	 /usr/include/stdio.h /usr/include/string.h \
		 /usr/include/ctype.h 

GLOBALINCS = /usr/include/ctype.h /usr/include/stdio.h /usr/include/string.h


clean:
	rm -f $(OBJECTS)
	
clobber:	
	rm -f $(OBJECTS) $(MAINS)

newmakefile:
	makefile -m -f $(MAKEFILE) 

#bottom#

makefile : makefile.sh
	cp makefile.sh makefile

mkxref : mkxref.sh
	cp mkxref.sh mkxref

nlorder : nlorder.sh
	cp nlorder.sh nlorder

