32ld netty.m i
send_netty -i 1 icons.d/jackdaniels
send_netty -i 2 icons.d/bigswitch1
send_netty -i 3 icons.d/bigswitch2
send_netty -i 4 icons.d/letter2
send_netty -p 1 1 "Jack" "echo No Thanks!\r"
send_netty -p 2 2 "# 1" "echo On\r"
send_netty -p 3 3 "# 2" "echo Off\r"
send_netty -p 4 4 "Write?" "echo Why Not Write\r"
